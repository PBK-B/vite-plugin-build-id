{
	"name": "@tzmax/vite-plugin-build-id",
	"version": "0.0.1",
	"type": "module",
	"author": "Bin <bin@PBK6.cn>",
	"license": "MIT",
	"description": "plugin for project generation build id",
	"keywords": [
		"vite",
		"version",
		"plugin",
		"vite-plugin"
	],
	"homepage": "https://github.com/PBK-B/vite-plugin-build-id.git",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PBK-B/vite-plugin-build-id.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown",
		"build:example": "vite build ./example",
		"lint": "eslint .",
		"prettier": "prettier -w .",
		"check:type": "tsc --noEmit",
		"check:lint": "npm run lint",
		"prepare": "husky .husky"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"commitlint": "^19.8.1",
		"eslint": "^9.32.0",
		"eslint-plugin-prettier": "^5.5.3",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"tsdown": "^0.13.0",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.38.0"
	},
	"peerDependencies": {
		"vite": "*"
	}
}
